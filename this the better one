    def mainmenu(self):
        print("1. Meal for 2 - 5 items.")
        print("2. Meal for 3 - 6 items.")
        print("3. Meal for 4 - 7 items.")
        choice = int(input("Please choose a meal deal. 1 to 3: "))
        
        
        if choice == 1:
            mealdeal = 5
            print("1. Crispy wonton. ")
            print("2. Hot and sour soup. ")
            print("3. Chicken with sweetcorn soup. ")
            choice = int(input("Please choose one starter. 1 to 3: "))
    
            if choice == 1:
    
                self.wonton = True
    
                if mealdeal == 5:
                    m.items5()
            
                elif mealdeal == 6:
                    m.items6()
            
                elif mealdeal == 7:
                    m.items7()
    
    
            
            elif choice == 2:
                self.hotsoursoup = True
                if mealdeal == 5:
                    m.items5()
            
                elif mealdeal == 6:
                    m.items6()
            
                elif mealdeal == 7:
                    m.items7()
            
    
        
            elif choice == 3:
                self.chickensweetcornsoup = True
    
                if mealdeal == 5:
                    m.items5()
            
                elif mealdeal == 6:
                    m.items6()
            
                elif mealdeal == 7:
                    m.items7()
    
    
        
                else:
                    print("That is not a choice!")
                    m.mainmenu()
        
        elif choice == 2:
            mealdeal = 6
            print("1. Crispy self.wonton. ")
            print("2. Hot and sour soup. ")
            print("3. Chicken with sweetcorn soup. ")
            choice = int(input("Please choose one starter. 1 to 3: "))
    
            if choice == 1:
                startermeal = "Crispy self.wonton"
                if mealdeal == 5:
                    m.items5()
            
                elif mealdeal == 6:
                    items6()
            
                elif mealdeal == 7:
                    items7()
        
        elif choice == 3:
            mealdeal = 7
            print("1. Crispy self.wonton. ")
            print("2. Hot and sour soup. ")
            print("3. Chicken with sweetcorn soup. ")
            choice = int(input("Please choose one starter. 1 to 3: "))
    
            if choice == 1:
                startermeal = "Crispy self.wonton"
                if mealdeal == 5:
                    m.items5()
            
                elif mealdeal == 6:
                    items6()
            
                elif mealdeal == 7:
                    items7()
    
    
            
            elif choice == 2:
                startermeal = "Hot and sour soup"
                if mealdeal == 5:
                    m.items5()
            
                elif mealdeal == 6:
                    items6()
            
                elif mealdeal == 7:
                    items7()
            
    
        
            elif choice == 3:
                startermeal = "Chicken with sweetcorn soup"
    
                if mealdeal == 5:
                    m.items5()
            
                elif mealdeal == 6:
                    items6()
            
                elif mealdeal == 7:
                    items7()
    
    
        
            else:
                print("That is not a choice!")
                return
        
        else:
            print("Thats not a choice!")
            sleep(2)
            mainmenu()
    
def Login():
    email = input("Please enter your email: ")
    password = input("Please enter your password: ")

    find_user = ("SELECT * FROM user WHERE email = ? AND password = ?")
    cursor.execute(find_user, [(email), (password)])
    results = cursor.fetchall()
    
    if results:
        for i in results:
            m.mainmenu()
            
        else:
            return

def Register():
    fname = input("Please enter your firstname: ")
    sname = input("Please enter you second name: ")
    mail = input("Please enter your email: ")
    npassword = input("Please enter a password: ")
    postcode = input("Please enter your postcode: ")
    city = input("Please enter the city you live in: ")
    house = input("Please enter your house number: ")
    address = input("Please enter your address: ")
    phone = int(input("Please enter your phone number: "))
   

    create_account = ("INSERT INTO user (firstname, secondname, email, password, postcode, city, housenum, addressline, phone) VALUES (?,?,?,?,?,?,?,?,?)")
    cursor.execute(create_account, [(fname), (sname), (mail), (npassword), (postcode), (city), (house), (address), (phone)])
    db.commit
    Login()

    print("Registered, please now Login!")

    
    
def LoginorRegister():
    print("1: Login")
    print("2: Register")
    choice = int(input("Please choose an option. 1 or 2: "))

    if choice == 1:
        Login()

    elif choice == 2:
        Register()
    
    elif choice != 1 or 2:
        print("That is not a choice! ")
        LoginorRegister()
    
    
m = menu()
LoginorRegister()   
    


    


